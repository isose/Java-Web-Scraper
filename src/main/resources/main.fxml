<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="TOP_CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.ScraperController">
   <children>
      <Text fill="#e8e8e8" strokeType="OUTSIDE" strokeWidth="0.0" text="Steam Game Scraper" textAlignment="CENTER">
         <font>
            <Font name="Calibri" size="42.0" />
         </font>
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin></Text>
      <Button fx:id="scrapeBtn" mnemonicParsing="false" onAction="#scrape" prefHeight="40.0" prefWidth="200.0" text="Scrape" textAlignment="CENTER">
         <VBox.margin>
            <Insets />
         </VBox.margin></Button>
       <fx:include fx:id="gameListView" source="GameListView.fxml" VBox.vgrow="ALWAYS" />
      <Region maxHeight="10.0" minHeight="10.0" />
   </children>
</VBox>
